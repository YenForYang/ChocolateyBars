    <id>cfr-decompiler</id>
    <version>0.132</version>
    <title>Class File Reader (CFR) Java Decompiler</title>
    <authors>Lee Benfield</authors>
    <owners>Richard Yang</owners>
    <licenseUrl>http://www.benf.org/other/cfr/license.html</licenseUrl>
